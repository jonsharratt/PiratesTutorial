{
  "build_type": "scala",
  "build_assets": ["gsim"],
  "generate_build_scripts": true,
  "launch": {
	"auto": {
		"windows": {
			"command": "sbt",
			"arguments": ["set fork in run := true", "runMain SpatialOSLauncherWithAutomaticWorkers"]
		},
		"mac": {
			"command": "sbt",
			"arguments": ["set fork in run := true", "runMain SpatialOSLauncherWithAutomaticWorkers"]
		}
	},
	"manual": {
		"windows": {
			"command": "sbt",
			"arguments": ["set fork in run := true", "runMain SpatialOSLauncherWithManualWorkers"]
		},
		"mac": {
			"command": "sbt",
			"arguments": ["set fork in run := true", "runMain SpatialOSLauncherWithManualWorkers"]
		}
	}
  }
}